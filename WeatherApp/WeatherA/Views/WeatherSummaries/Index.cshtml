@model WeatherA.Models.WeatherSummary

@{
    ViewBag.Title = "Index";
}
<main>
    <div class="container-fluid">
        <h1 class="mt-4">Variables</h1>
        <div class="card mb-4">
            <div class="card-header"><i class="fas fa-table mr-1"></i>Temperatura</div>
            <div class="card-body">
                @Html.DisplayFor(model => model.temp)
            </div>
        </div>
        <div class="card mb-4">
            <div class="card-header"><i class="fas fa-table mr-1"></i>Humedad</div>
            <div class="card-body">
                @Html.DisplayFor(model => model.humidity)
            </div>
        </div>
        <div class="card mb-4">
            <div class="card-header"><i class="fas fa-table mr-1"></i>Pluviometria</div>
            <div class="card-body">
                @Html.DisplayFor(model => model.precipRate)
            </div>
        </div>
    </div>
</main>/p>